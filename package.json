{
  "name": "whatsapp-web",
  "version": "1.0.0",
  "description": "",
  "main": "server.js",
  "scripts": {
    "build-ts": "tsc",
    "build": "yarn run build-ts && yarn run lint",
    "lint": "tsc --noEmit && eslint \"**/*.{js,ts}\" --quiet --fix",
    "serve-debug": "nodemon --inspect dist/server.js",
    "serve": "node --max-old-space-size=65536 dist/server.js",
    "start": "yarn run serve",
    "test": "jest --forceExit --coverage --verbose",
    "watch-debug": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"cyan.bold,green.bold\" \"yarn run watch-ts\" \"yarn run serve-debug\"",
    "watch-node": "nodemon src/server.ts",
    "watch-test": "yarn run test -- --watchAll",
    "watch-ts": "tsc -w",
    "watch": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"cyan.bold,green.bold\" \"yarn run watch-ts\" \"yarn run watch-node\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kar-tini/whatsapp-bot"
  },
  "author": "aulia rafi",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/kar-tini/whatsapp-bot/issues"
  },
  "homepage": "https://github.com/kar-tini/whatsapp-bot#readme",
  "dependencies": {
    "axios": "^0.24.0",
    "body-parser": "^1.20.1",
    "compression": "^1.7.4",
    "dotenv": "^16.0.0",
    "errorhandler": "^1.5.1",
    "express": "^4.18.2",
    "http": "^0.0.1-security",
    "ioredis": "^4.28.2",
    "lusca": "^1.7.0",
    "moment": "^2.29.1",
    "qrcode": "^1.5.1",
    "qrcode-terminal": "^0.12.0",
    "socket.io": "^4.5.4",
    "whatsapp-web.js": "^1.18.4"
  },
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/body-parser": "^1.19.2",
    "@types/compression": "^1.7.2",
    "@types/concurrently": "^6.4.0",
    "@types/dotenv": "^8.2.0",
    "@types/errorhandler": "^1.5.0",
    "@types/eslint": "^8.2.1",
    "@types/express": "^4.17.14",
    "@types/ioredis": "^4.28.5",
    "@types/jest": "^27.4.0",
    "@types/lusca": "^1.7.1",
    "@types/moment": "^2.13.0",
    "@types/node": "^17.0.6",
    "@types/node-fetch": "^2.6.2",
    "@types/qrcode": "^1.5.0",
    "@types/qrcode-terminal": "^0.12.0",
    "@typescript-eslint/eslint-plugin": "^5.8.1",
    "@typescript-eslint/parser": "^5.8.1",
    "concurrently": "^7.0.0",
    "eslint": "^8.6.0",
    "jest": "^27.4.5",
    "nodemon": "^2.0.15",
    "ts-jest": "^27.1.2",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.4"
  }
}
